    "pinia": "^2.1.7",
    "promise.prototype.finally": "^3.1.7",
    "unocss-preset-scrollbar": "^0.3.0",
    "unplugin-vue-components": "^0.26.0",
    "vconsole": "^3.15.1",
    "vue": "^3.3.11",
    "vue-draggable-plus": "^0.3.5",
    "vue-i18n": "^9.9.0",
